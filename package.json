{
  "name": "fleetrack",
  "version": "2.0.0",
  "description": "A visualization of real-time flight data for a handful of major carriers (i.e. fleet tracking) on an scalable vector altitude/distance graph using the D3.js visualization library.",
  "repository": {
    "type": "git",
    "url": "git://github.com/jeffgore00/fleetrack"
  },
  "main": "main.js",
  "scripts": {
    "build-client": "webpack --mode=production",
    "test": "NODE_ENV=testing mocha test -r @babel/polyfill -r @babel/register -r jsdom-global/register",
    "start": "node main.js",
    "start-dev": "webpack -w --mode=development & NODE_ENV=development nodemon main.js --ignore 'public/bundle.js' --ignore 'public/bundle.map.js'",
    "start-test": "webpack -w --mode=development & NODE_ENV=testing nodemon main.js --ignore 'public/bundle.js' --ignore 'public/bundle.map.js'",
    "deploy": "script/deploy"
  },
  "keywords": [],
  "author": "Jeff Gore",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "d3-axis": "^1.0.8",
    "d3-scale": "^2.1.0",
    "d3-selection": "^1.3.0",
    "d3-transition": "^1.1.1",
    "d3-zoom": "^1.7.1",
    "express": "^4.16.3",
    "morgan": "^1.9.1",
    "path": "^0.12.7",
    "pg": "^7.6.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "request": "^2.88.0",
    "sequelize": "^4.41.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "axios-mock-adapter": "^1.15.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.1.2",
    "jsdom": "11.12.0",
    "jsdom-global": "3.0.2",
    "mocha": "^5.2.0",
    "redux-mock-store": "^1.5.3",
    "sinon": "^6.1.4",
    "webpack": "^4.6.0",
    "webpack-cli": "^3.1.2"
  }
}
